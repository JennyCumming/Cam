{#
/**
 * Part of the expressive compact callout to point to information in a visually appealing way.
 * This callout displays up to 2 images, any third image will be ignored
 *
 */
#}
<div class="grid grid-cols-1 md:grid-cols-2 gap-[20px] p-[36px]">
     {% set count = '1' %}
     {% for image in image_multiple %}
          {% set image_uri = file_url(image.uri) %}
          {% if count == '1' %}
               <img src="{{ image_uri }}" alt="{{ image_alt }}" class="object-cover h-[288px] w-[100%] rounded">
          {% elseif count == '2' %}<div class="flex flex-col justify-end">
               <img src="{{ image_uri }}" alt="{{ image_alt }}" class="object-cover h-[176px] w-[100%] rounded"></div>
          {% endif %}
          {% set count = count + 1 %}
     {% endfor %}
</div>
