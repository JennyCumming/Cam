{#
/**
 * @file
 * Default theme implementation for a calendar pager.
 *
 * Available variables:
 * - items: List of pager items.
 *
 * @ingroup themeable
 */
#}

{% if (items.previous or items.next) and not exclude %}
    <ul class="pager">
      {% if items.previous %}
      <li class="date-prev">
        <a href="{{ items.previous.url }}" title="{{ 'Go to previous page'|t }}" rel="prev"{{ items.previous.attributes|without('href', 'title', 'rel') }}>
          <span aria-hidden="true">{{ items.previous.text|default('&laquo;'|t) }}</span>
          {% if (not hide_text) %}<span class="nav_text">{{ 'Prev'|t }}</span>{% endif %}
        </a></li>
      {% endif %}
      {% if items.next %}
      <li class="date-next">
        <a href="{{ items.next.url }}" title="{{ 'Go to next page'|t }}" rel="next"{{ items.next.attributes|without('href', 'title', 'rel') }}>
          {% if (not hide_text) %}<span class="nav_text">{{ 'Next'|t }}</span>{% endif %}
          <span aria-hidden="true">{{ items.next.text|default('&raquo;'|t) }}</span>
        </a></li>
      {% endif %}
    </ul>
{% endif %}