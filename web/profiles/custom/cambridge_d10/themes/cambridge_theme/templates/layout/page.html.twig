    <div class="campl-row campl-content{% if page.carousel or page.pre_content %} campl-recessed-content{% endif %}">
      <div class="campl-wrap clearfix">
        {% set columns = 12 %}
        {% if page.left_navigation %}
          {% set columns = columns - 3 %}
        {% endif %}
        {% if page.sub_content %}
          {% set columns = columns - 3 %}
        {% endif %}
        {% if has_sidebar %}
          {% set columns = columns - 3 %}
        {% endif %}

        {% if page.left_navigation %}
        <div class="campl-column3">
          <div class="campl-tertiary-navigation" style="min-height: 970px;">
              <div class="campl-tertiary-navigation-structure">
                <ul class="campl-unstyled-list campl-vertical-breadcrumb"><li class="leaf menu-mlid-316"><a href="/">{{ site_name }}<span class="campl-vertical-breadcrumb-indicator"></span></a></li></ul>
